<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <remote name="ok"
	    fetch="https://github.com/OpenKirin-Treble/"
	    revision="android-8.0" />

    <project path="build" name="platform_build" remote="ok">
	<copyfile dest="Makefile" src="core/root.mk" />
	<linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
    	<linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
    	<linkfile dest="build/core" src="core" />
    	<linkfile dest="build/envsetup.sh" src="envsetup.sh" />
    	<linkfile dest="build/target" src="target" />
    	<linkfile dest="build/tools" src="tools" />
    </project>
    <project path="frameworks/base" name="platform_frameworks_base" remote="ok" />
    <project path="frameworks/native" name="platform_frameworks_native" remote="ok" />
    <project path="frameworks/opt/net/wifi" name="platform_frameworks_opt_net_wifi" remote="ok" />
    <project path="packages/services/Telephony" name="platform_packages_services_Telephony" remote="ok" />
    <project path="system/bt" name="platform_system_bt" remote="ok" />
    <project path="system/core" name="platform_system_core" remote="ok" />
    <project path="system/netd" name="platform_system_netd" remote="ok" />
    <project path="system/vold" name="platform_system_vold" remote="ok" />

</manifest>
